# Agent Configuration
AGENT_APP=AdbAgent

# Logging Configuration  
LOG_LEVEL=INFO

# Microsoft App Registration (required for Agent Framework SDK integration)
MICROSOFT_APP_ID=
MICROSOFT_APP_PASSWORD=
MICROSOFT_APP_TENANT_ID=

# Service Connection Settings (required for OBO token exchange)
CONNECTIONS__SERVICE_CONNECTION__SETTINGS__CLIENTID=
CONNECTIONS__SERVICE_CONNECTION__SETTINGS__CLIENTSECRET=
CONNECTIONS__SERVICE_CONNECTION__SETTINGS__TENANTID=
 
# Agent Application Authorization Settings
AGENTAPPLICATION__USERAUTHORIZATION__HANDLERS__GRAPH__SETTINGS__AZUREBOTOAUTHCONNECTIONNAME=OauthBotAppConnection
AGENTAPPLICATION__USERAUTHORIZATION__HANDLERS__GRAPH__SETTINGS__OBOCONNECTIONNAME=SERVICE_CONNECTION
 
DATABRICKS_HOST=

FOUNDRY_URL=
ADB_CONNECTION_NAME=
MODEL_DEPLOYMENT_NAME=

# Agent Framework SDK Configuration
# The Agent Framework uses the service connection credentials above with Azure AI scope (https://ai.azure.com/.default)
AZURE_AI_MODEL_DEPLOYMENT_NAME=

# Azure AI Authentication Notes:
# - Uses SERVICE_CONNECTION credentials with proper Azure AI scope
# - FOUNDRY_URL should be your Azure AI project endpoint
# - MODEL_DEPLOYMENT_NAME should match your deployed model name

STORAGE_ACCTNAME=
STORAGE_CONTNAME=